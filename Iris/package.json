    "@mui/icons-material": "^6.4.4",
    "@mui/material": "^6.4.4",
    "@mui/styled-engine": "^7.0.2",
    "axios": "^1.8.3",
    "axios-cookiejar-support": "^5.0.5",
    "cryptojs": "^2.5.3",
    "electron-squirrel-startup": "^1.0.1",
    "electron-store": "^10.0.1",
    "iris": "file:",
    "lucide-react": "^0.487.0",
