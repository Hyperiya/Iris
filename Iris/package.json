{
  "name": "@hyperiya/iris",
  "productName": "Iris",
  "version": "1.0.0",
  "description": "Music & Game Stat Displayer",
  "main": ".webpack/main/index.js",
  "build": {
    "appId": "hyperiya.app.iris",
    "win": {
      "icon": "src/assets/icons/Iris.ico",
      "target": [
        "nsis"
      ]
    },
    "mac": {
      "icon": "assets/icons/icon.icns"
    },
    "linux": {
      "icon": "assets/icons/icon.png"
    }
  },
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "build:win": "electron-forge make --platform win32",
    "build:mac": "electron-forge make --platform darwin",
    "build:linux": "electron-forge make --platform linux",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .ts,.tsx .",
    "postinstall": "electron-rebuild"
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.6.1",
    "@electron-forge/maker-deb": "^7.6.1",
    "@electron-forge/maker-dmg": "^7.8.0",
